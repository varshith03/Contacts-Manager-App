<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#FFFFFF" /> <!-- Background color (white) -->
            <corners android:radius="4dp" /> <!-- Rounded corners -->
            <stroke android:color="#000000" android:width="2dp" /> <!-- Border color (black) and width -->
        </shape>
    </item>


</layer-list>
